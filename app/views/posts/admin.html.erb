
	<%= link_to 'New', new_post_path %>
	<% @posts.each do |p|%>
	<div class="row span10">
		<h4><%= p.title %></h4>
		<p><%= p.id %></p>
		<p><%= p.body %></p>
		<p><%= p.image.url %></p>
		<%= link_to 'Edit', edit_post_path(p) %>
		<%= link_to 'Destroy', p, :confirm => 'Are you sure?', :method => :delete %>
	</div>
	<% end %>



